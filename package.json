{
  "name": "obsidian-to-mp",
  "version": "1.0.0",
  "description": "Convert Obsidian markdown to WeChat MP format",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "builtin-modules": "^4.0.0",
    "esbuild": "^0.25.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "obsidian": "latest"
  }
}
